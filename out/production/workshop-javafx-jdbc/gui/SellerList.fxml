<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="415.0" prefWidth="764.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SellerListController">
   <children>
      <Label text="Registro de vendedores">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <padding>
            <Insets left="5.0" top="5.0" />
         </padding>
      </Label>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button fx:id="btNew" mnemonicParsing="false" onAction="#onBtNewAction" text="Novo Vendedor" />
        </items>
      </ToolBar>
      <TableView fx:id="tableViewSeller" prefHeight="200.0" prefWidth="695.0">
        <columns>
          <TableColumn fx:id="tableColumnId" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tableColumnNome" prefWidth="75.0" text="Nome" />
            <TableColumn fx:id="tableColumneMAIL" prefWidth="75.0" text="Email" />
            <TableColumn fx:id="tableColumnBirthDate" prefWidth="176.0" text="Data de nascimento" />
            <TableColumn fx:id="tableColumnBaseSalary" prefWidth="69.0" text="Salario" />
            <TableColumn fx:id="tableColumnEDIT" prefWidth="77.0" />
            <TableColumn fx:id="tableColumnREMOVE" prefWidth="83.0" />
        </columns>
      </TableView>
   </children>
</VBox>
